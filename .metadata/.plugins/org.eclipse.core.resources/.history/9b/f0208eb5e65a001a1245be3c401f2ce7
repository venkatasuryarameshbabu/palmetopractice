package Ex1;

import java.util.ArrayList;

public class UserMain {
public static int compareDashes (String s1,String s2)
{
	for(int i=0;i<s1.length();i++)
	{
		if(s1.charAt(i)=='-')
		{
		  for(int j=0;j<s2.length();j++)
		  {
			  if(s2.charAt(i)=='-')
				  return 1;
		  }
		  
		}	
	}
	return 2;
}
}
